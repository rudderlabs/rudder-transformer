PORT=9090 # application's port
METRICS_PORT=9091 # metrics's port
ENABLE_FUNCTIONS=true # user transformation functions
CLUSTER_ENABLED=true # cluster mode
NUM_PROCS=1 # Number of workers in the cluster mode
CONFIG_BACKEND_URL="https://api.rudderstack.com"
REDIS_HOST = localhost
REDIS_PORT = 6379
REDIS_PASSWORD = 123
REDIS_USERNAME = abc
USE_REDIS_DB = true
REDIS_EXPIRY_TIME_IN_SEC = 3600

# Feature Flag Configuration
FEATURE_FLAG_ENABLED=true # Enable/disable feature flag service
FEATURE_FLAG_PROVIDER=flagsmith # Provider: flagsmith or local
FEATURE_FLAG_API_KEY=your_api_key # Flagsmith API key (required for flagsmith provider)
FEATURE_FLAG_ENABLE_CACHE=true # Enable caching for better performance
FEATURE_FLAG_CACHE_TTL_SECONDS=10 # Cache TTL in seconds (10 seconds)
FEATURE_FLAG_ENABLE_LOCAL_EVALUATION=false # Enable if only one time pull is needed
