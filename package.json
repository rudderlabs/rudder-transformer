{
  "name": "rudder-transformer",
  "version": "1.19.0",
  "description": "",
  "homepage": "https://github.com/rudderlabs/rudder-transformer#readme",
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-transformer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudderlabs/rudder-transformer.git"
  },
  "license": "ISC",
  "author": "",
  "main": "GATransform.js",
  "scripts": {
    "setup": "npm ci",
    "format": "prettier --write .",
    "lint": "eslint . || exit 0",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint --ext .js,.json . --fix; exit 0",
    "lint:fix:json": "eslint --ext .json --fix .",
    "check:merge": "npm run verify || exit 1; codecov",
    "start": "node src/index.js",
    "test:versionedRouter:all": "jest versionedRouter --type=all --detectOpenHandles --coverage --notify --watchAll=false",
    "test:versionedRouter": "jest versionedRouter --destName=heap --type=all --notify --detectOpenHandles --coverage --watchAll=false",
    "verify": "eslint . || exit 1; npm run test || exit 1",
    "test:testRouter": "jest testRouter --detectOpenHandles --coverage --notify --watchAll=false",
    "test": "jest --detectOpenHandles --coverage --notify --watchAll=false",
    "test:ci": "jest --detectOpenHandles --coverage --notify --expand --silent --watchAll=false --maxWorkers=2",
    "test:silent": "npm run test -- --silent",
    "test:benchmark": "node benchmark/index.js",
    "test:integration": "jest -c jest.config.integration.js",
    "pre-commit": "npm run test:silent && npx lint-staged",
    "commit-msg": "commitlint --edit",
    "prepare": "husky install",
    "release": "npx standard-version",
    "release:github": "DEBUG=conventional-github-releaser npx conventional-github-releaser -p angular --config github-release.config.js"
  },
  "dependencies": {
    "@amplitude/ua-parser-js": "0.7.24",
    "@aws-sdk/client-lambda": "^3.295.0",
    "@aws-sdk/client-s3": "^3.295.0",
    "@aws-sdk/lib-storage": "^3.295.0",
    "@bugsnag/js": "^7.20.0",
    "@koa/router": "^12.0.0",
    "@ndhoule/extend": "^2.0.0",
    "ajv": "^8.12.0",
    "ajv-draft-04": "^1.0.0",
    "ajv-formats": "^2.1.1",
    "aws-sdk": "^2.1339.0",
    "axios": "^1.3.4",
    "btoa": "^1.2.1",
    "cd": "^0.3.3",
    "component-each": "^0.2.6",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "flat": "^5.0.2",
    "form-data": "^4.0.0",
    "get-value": "^3.0.1",
    "handlebars": "^4.7.7",
    "http-graceful-shutdown": "^3.1.13",
    "https-proxy-agent": "^5.0.1",
    "is": "^3.3.0",
    "is-ip": "^3.1.0",
    "isolated-vm": "4.4.2",
    "js-sha1": "^0.6.0",
    "json-diff": "^1.0.3",
    "json-query": "^2.2.2",
    "jsontoxml": "^1.0.1",
    "jszip": "^3.10.1",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.4.0",
    "lodash": "^4.17.21",
    "match-json": "^1.3.5",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.41",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.9",
    "object-hash": "^3.0.0",
    "parse-static-imports": "^1.1.0",
    "pprof": "^3.2.0",
    "prom-client": "^14.2.0",
    "rudder-transformer-cdk": "^1.4.5",
    "rudder-workflow-engine": "^0.4.4",
    "set-value": "^4.1.0",
    "sha256": "^0.2.0",
    "stacktrace-parser": "^0.1.10",
    "statsd-client": "^0.4.7",
    "truncate-utf8-bytes": "^1.0.2",
    "ua-parser-js": "^1.0.34",
    "unset-value": "^2.0.1",
    "uuid": "^9.0.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.0",
    "acorn": "^8.8.2",
    "benchmark-suite": "^0.1.8",
    "commander": "^10.0.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.4.4",
    "conventional-github-releaser": "^3.1.5",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-date-mock": "^1.0.8",
    "jest-when": "^3.5.2",
    "lint-staged": "^13.2.0",
    "madge": "^6.0.0",
    "mocked-env": "^1.3.5",
    "node-notifier": "^10.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.6",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{json,js,ts,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "jiraMode": false,
      "jiraOptional": true,
      "skipScope": true,
      "defaultType": "feat"
    }
  }
}
